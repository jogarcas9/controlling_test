{
  "name": "controling_v3",
  "version": "1.0.0",
  "description": "Aplicaci√≥n de finanzas personales",
  "main": "server/server.js",
  "engines": {
    "node": "18.x",
    "npm": "9.x"
  },
  "scripts": {
    "start": "node server/server.js",
    "build": "node setup-env.js && cd client && npm install --legacy-peer-deps && npm install recharts --save && npm run build",
    "server": "cd server && nodemon server.js",
    "client": "cd client && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install --legacy-peer-deps && npm install recharts --save",
    "vercel-build": "npm run install-all && npm run build"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.8.4",
    "concurrently": "^8.2.0",
    "fs": "0.0.1-security",
    "path": "^0.12.7",
    "recharts": "^2.15.3"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/preset-env": "^7.27.1",
    "@babel/preset-react": "^7.27.1",
    "@eslint/js": "^9.26.0",
    "eslint": "^9.26.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.0.0",
    "nodemon": "^3.1.9"
  }
}
